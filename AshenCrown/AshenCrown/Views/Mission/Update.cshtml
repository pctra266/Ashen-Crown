@model AshenCrown.Domain.Entities.Mission
<h1>Update Mission</h1>
<form method="post">
    <div  >
        <input hidden asp-for="Id" />
    </div>

    <div>
        <label asp-for="Content"></label>
        <input asp-for="Content" />
        <span asp-validation-for="Content" />
    </div>
    <div>
        <label asp-for="IsComplete"></label>
        <input asp-for="IsComplete" />

    </div>
    <button type="submit">Submit</button>
</form>